    "module": "dist/esnext/index.js",
    "typings": "dist/esnext/index.d.ts",
    "repository": "ardeois/graphql-codegen-typescript-mock-data",
    "author": {
        "name": "Corentin Ardeois",
        "email": "corentin.ardeois@gmail.com",
